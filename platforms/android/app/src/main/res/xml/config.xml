<?xml version='1.0' encoding='utf-8'?>
<widget id="com.seacowtech.covid19" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Coronavirus (COVID-19)</name>
    <description>
        H5 App that track Coronavirus (COVID-19) status in Australia
    </description>
    <author email="cs.pangge@gmail.com" href="https://pangge.me">
        Ran Tao
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="hooks/vue-build.js" type="before_compile" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
